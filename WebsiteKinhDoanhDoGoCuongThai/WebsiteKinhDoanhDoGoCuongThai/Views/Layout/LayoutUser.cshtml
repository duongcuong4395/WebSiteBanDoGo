    <link href="~/Content/CssTemplate/prettyPhoto.css" rel="stylesheet" />
    <link href="~/Content/CssTemplate/price-range.css" rel="stylesheet" />
    <link href="~/Content/CssTemplate/animate.css" rel="stylesheet" />
